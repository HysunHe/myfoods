create or replace function get_geometry_oo (json varchar2)
return sdo_geometry deterministic is
begin
  return sdo_geometry (
    2001, 4326, 
    sdo_point_type (json_value(json, '$.LONGITUDE'),json_value(json, '$.LATITUDE'),null),
    null, null
  );
end;
